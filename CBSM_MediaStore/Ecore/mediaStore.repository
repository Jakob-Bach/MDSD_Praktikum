    <signatures name="HTTPUpload"/>
  </interfaces>
  <interfaces name="MediaStore">
    <signatures name="download"/>
    <signatures name="upload"/>
  </interfaces>
  <interfaces name="Sound">
    <signatures name="watermark"/>
  </interfaces>
  <interfaces name="AudioDB">
